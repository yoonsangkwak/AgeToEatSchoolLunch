<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AgeToEatSchoolLunch" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/main_one</item>
        <item name="colorPrimaryVariant">@color/main_two</item>
        <item name="colorOnPrimary">@color/main_three</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!-- DatePickerDialog -->
        <item name="colorAccent">@color/main_one</item>
        <item name="colorControlActivated">@color/main_one</item>
        <item name="colorControlHighlight">@color/main_two</item>
    </style>
</resources>